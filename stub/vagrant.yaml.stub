---
ip: "192.168.10.10"
memory: 2048
cpus: 1
hostname: vagrant-php70
name: vagrant-php70
provider: virtualbox

authorize: ~/.ssh/id_rsa.pub

keys:
    - ~/.ssh/id_rsa

folders:
    - map: ~/Code
      to: /home/vagrant/Code
sites:
    - map: vagrant-php70.app
      to: "/home/vagrant/Code/public"

